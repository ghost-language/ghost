class CoffeeMaker {
    function constructor() {
        this.coffee := "coffee"
    }

    function brew() {
        print('Enjoy your cup of ' + this.coffee)
    }
}

class EspressoMaker extends CoffeeMaker {
    function constructor() {
        this.coffee := "espresso"
    }
}

coffee := CoffeeMaker()
coffee.brew()

espresso := EspressoMaker()
espresso.brew()