class CoffeeMaker {
    coffee := "espresso"

    function constructor() {
        print("coffee maker constructor called")
    }

    function calibrate() {
        print("Calibrating machine...")

        return this
    }

    function brew() {
        this.coffee := "abc"
        print("Brewing " + this.coffee + "...")
        print("Enjoy.")
    }
}

maker := CoffeeMaker()

maker.calibrate().brew()