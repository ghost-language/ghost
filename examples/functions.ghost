function Hello(closure) {
    print("Hello world!")

    closure()
}

foobar := function(name = 'world', bool = false) {
    print('Hello, ' + name + '!')
    print(bool)
}

function makeCoffee(type = 'cappuccino') {
    print('Making a cup of ' + type)
}

hello(function() {
    print("I'm an anonymous function being passed through another function!")
})

foobar()
makeCoffee()
makeCoffee('coffee')
makeCoffee('espresso')